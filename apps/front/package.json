		"prettier-plugin-tailwindcss": "^0.6.14",
		"sass-embedded": "^1.89.2",
		"svelte": "5.36.5",
		"svelte-check": "^4.2.2",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.37.0",
		"vite": "^7.0.5"
	},
	"dependencies": {
		"@elysiajs/eden": "^1.3.2",
		"@hoe/assets": "workspace:*",
		"@hoe/system": "workspace:*",
		"@iconify/svelte": "^5.0.0",
		"@libsql/client": "^0.15.10",
		"drizzle-orm": "^0.44.3",
		"jsonwebtoken": "^9.0.2",
		"socket.io-client": "^4.8.1",
